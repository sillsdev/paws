	   	<radio id="NPPossPronAfter" checked="yes" dataValue="after">Après le nom</radio>
	   	<radio id="NPPossPronEitherSide" checked="yes" dataValue="either">D’un côté ou l’autre du nom</radio>
	   </radioGroup>

	   <radioGroup id="NPPossPPPosTable" show="swNPPossNPNotYesOnly">
