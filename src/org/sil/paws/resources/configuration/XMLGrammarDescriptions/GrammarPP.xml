        <or/>
        <whenValue element="typology" attr="case" value="ergative" negateValue="yes"/>
        <and/>
        <whenValue element="pp" attr="pPos" value="both"/>
        <and/>
        <whenValue element="pp" attr="pType" value="word"/>
